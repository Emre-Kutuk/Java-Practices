package creditClick;

public class Path 
{
	public Town from;
	public Town to;
	public int distance;
	
	public Path(Town from, Town to, int distance)
	{
		this.from = from;
		this.to = to;
		this.distance = distance;
	}
	
	int i = 3; 
	String s = i.ToString();
	//overriding toString method
	public String toString()
	{
		return "";
	}	
}
